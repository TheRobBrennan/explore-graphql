{
  "name": "explore-graphql",
  "version": "0.0.0",
  "author": "Rob Brennan <rob@therobbrennan.com> (therobbrennan.com)",
  "description": "A starting point to explore GraphQL",
  "scripts": {
    "start": "npm run docker:apollo-tutorial:up",
    "docker:apollo-tutorial:up": "docker-compose -f docker-compose.apollo-tutorial.yml up",
    "docker:apollo-tutorial:up:clean": "docker-compose -f docker-compose.apollo-tutorial.yml up --remove-orphans --build",
    "docker:apollo-tutorial:down": "docker-compose -f docker-compose.apollo-tutorial.yml down",
    "docker:example-client-server-with-mongodb:up": "docker-compose -f docker-compose.example-client-server-with-mongodb.yml up",
    "docker:example-client-server-with-mongodb:up:clean": "docker-compose -f docker-compose.example-client-server-with-mongodb.yml up --remove-orphans --build",
    "docker:example-client-server-with-mongodb:down": "docker-compose -f docker-compose.example-client-server-with-mongodb.yml down",
    "docker:example-client-server-with-postgresql:up": "docker-compose -f docker-compose.example-client-server-with-postgresql.yml up",
    "docker:example-client-server-with-postgresql:up:clean": "docker-compose -f docker-compose.example-client-server-with-postgresql.yml up --remove-orphans --build",
    "docker:example-client-server-with-postgresql:down": "docker-compose -f docker-compose.example-client-server-with-postgresql.yml down",
    "docker:nuke": "npm run docker:nuke:containers && docker rmi -f $(docker images -q)",
    "docker:nuke:containers": "docker rm $(docker ps -aq)"
  },
  "keywords": [
    "graphql"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/explore-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/TheRobBrennan/explore-graphql/issues"
  },
  "homepage": "https://github.com/TheRobBrennan/explore-graphql#readme"
}
